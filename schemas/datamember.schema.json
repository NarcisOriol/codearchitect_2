{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "DataMember",
  "format": "parent-object",
  "contentMediaType": "symbol-field",
  "properties": {
    "$id": {
      "type": "string",
      "format": "hidden"
    },
    "$label": {
      "type": "string",
      "format": "hidden"
    },
    "datatype": {
      "format": "dropdown-select-tag",
      "$ref": "./tags.schema.json",
      "const": ["types"]
    },
    "isArray": {
      "format": "input-string",
      "type": "string"
    },
    "isPointer": {
      "format": "checkbox",
      "type": "boolean"
    },
    "isVolatile": {
      "format": "checkbox",
      "type": "boolean"
    },
    "isConst": {
      "format": "checkbox",
      "type": "boolean"
    }
  }
}