{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Project",
  "contentMediaType": "symbol-method",
  "type": "object",
  "format": "parent-object",
  "properties": {
    "$id": {
      "type": "string",
      "format": "hidden"
    },
    "$label": {
      "type": "string",
      "format": "hidden"
    },
    "Classes": {
      "format": "array-parent-objects",
      "contentMediaType": "folder",
      "type": "array",
      "items": {
        "$ref": "./class.schema.json"
      }
    },
    "Requirements": {
      "format": "array-parent-objects",
      "contentMediaType": "folder",
      "type": "array",
      "items": {
        "$ref": "./reqs.schema.json"
      }
    },
    "Primitive Types": {
      "format": "array-parent-objects",
      "contentMediaType": "folder",
      "type": "array",
      "items": {
        "$ref": "./primitivetypes.schema.json"
      }
    },
    "$tags": {
      "type": "array",
      "format": "hidden",
      "items": {
        "type": "object",
        "properties": {
          "$tag": {
            "type": "array",
            "items": {
              "$ref": "./tags.schema.json"
            }
          },
          "$label": {
            "type": "string"
          },
          "$id": {
            "type": "string"
          }
        }
      }
    }
  }
}