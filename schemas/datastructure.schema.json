{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "DataStructure",
  "format": "parent-object",
  "contentMediaType": "symbol-struct",
  "properties": {
    "$id": {
      "type": "string",
      "format": "hidden"
    },
    "$label": {
      "type": "string",
      "format": "hidden"
    },
    "type": {
      "format": "dropdown-select",
      "type": "string",
      "enum": ["struct", "union"]
    },
    "members": {
      "contentMediaType": "folder",
      "format": "array-parent-objects",
      "type": "array",
      "items": {
        "$ref": "./datamember.schema.json"
      }
    },
    "$tag": {
      "format": "hidden",
      "type": "array",
      "const": [
        "*",
        "types"
      ]
    }
  }
}